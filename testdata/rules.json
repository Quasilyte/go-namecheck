{
    "^context\\.Context$|^\\w+Context$|^[Cc]ontext$": {
        "param+field": {"ctx": "ctxt"}
    },

    "regexp\\.Regexp": {
        "local+global": {
            "^re[A-Z]\\w*$": "use RE suffix instead of re prefix"
        }
    },

    "testing.T": {"param": {".*": "use t for testing.T params"}},
    "testing.B": {"param": {".*": "use b for testing.B params"}},

    "^func\\(.*?\\)": {
        "param": {"f": "fn"}
    }
}
